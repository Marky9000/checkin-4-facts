function huiswerkMaken(vak, callback) {
    console.log(`Ok,... Ik ga nu mijn ${vak} huiswerk maken.`);
    setTimeout(()=> {
                callback();
         }, 4000);
 
  }
function klaarMetHuiswerk(){
    console.log('Kijk Paps & Mams ik ben Klaar!')
}
huiswerkMaken('wiskunde', klaarMetHuiswerk );
